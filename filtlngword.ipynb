        "##write a function filter_long-words\r\n",
        "def filter_long_words(words,n):\r\n",
        "  l=list()\r\n",
        "  for i in words:\r\n",
        "    if len(i) > n:\r\n",
        "      l.append(i)\r\n",
        "      print(f\"longest words are: {l}\")"
      ],
      "execution_count": 6,
      "outputs": []
